{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "drizzle:studio": "drizzle-kit studio --host 127.0.0.1",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint --fix \"src/**/*.ts*\"",
    "postinstall": "npm run db:generate && npm run db:migrate",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.fastify",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/core": "^0.34.1",
    "@builder.io/qwik": "1.5.7",
    "@builder.io/qwik-auth": "0.2.2",
    "@builder.io/qwik-city": "1.5.7",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.22.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qwik": "1.5.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "3.4.4",
    "tsx": "^4.16.2",
    "typescript": "5.5.3",
    "undici": "^6.19.2",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.3",
    "@fastify/static": "^7.0.4",
    "@fontsource/geist-sans": "^5.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "fast-glob": "^3.3.2",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "generate-api-key": "^1.0.2",
    "mini-svg-data-uri": "^1.4.4",
    "pino-roll": "^1.1.0",
    "postgres": "^3.4.4",
    "qwik-sonner": "^1.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.33.4",
    "simple-git": "^3.25.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.3.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yaml": "^2.4.5"
  }
}
